<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- 医疗十字符号 -->
    <group android:translateX="54" android:translateY="54">
        <!-- 十字的垂直部分 -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M-4,-20 L4,-20 L4,20 L-4,20 Z" />
        <!-- 十字的水平部分 -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M-20,-4 L20,-4 L20,4 L-20,4 Z" />
    </group>
    
    <!-- 听诊器图标 -->
    <group android:translateX="54" android:translateY="54">
        <!-- 听诊器头部圆形 -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M-21,-15 A6,6 0 1,1 -9,-15 A6,6 0 1,1 -21,-15 Z" />
        
        <!-- 听诊器管道 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#FFFFFF"
            android:strokeWidth="2"
            android:strokeLineCap="round"
            android:pathData="M-15,-9 Q-10,0 -5,8 Q0,15 8,15" />
        
        <!-- 听诊器耳塞圆形 -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M5,15 A3,3 0 1,1 11,15 A3,3 0 1,1 5,15 Z" />
    </group>
    
    <!-- 心电图波形 -->
    <path
        android:fillColor="#00000000"
        android:strokeColor="#FFFFFF"
        android:strokeWidth="2"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"
        android:pathData="M25,75 L30,75 L32,65 L34,85 L36,55 L38,75 L45,75" />
    
    <!-- 药片图标 -->
    <group android:translateX="75" android:translateY="35">
        <!-- 药片椭圆形 -->
        <path
            android:fillColor="#FFFFFF"
            android:pathData="M-8,0 A8,5 0 1,1 8,0 A8,5 0 1,1 -8,0 Z" />
        <!-- 药片分割线 -->
        <path
            android:fillColor="#00000000"
            android:strokeColor="#4A90E2"
            android:strokeWidth="1"
            android:pathData="M-8,0 L8,0" />
    </group>
</vector>